    "version": "0.2.0",
    "configurations": [
        {
            "name": "eminelstandard-app",
            "request": "launch",
            "type": "dart",
            "args": ["--dart-define-from-file=.env"],
        },
        {
            "name": "eminelstandard-app (profile mode)",
            "request": "launch",
            "type": "dart",
